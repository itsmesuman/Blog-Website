<%- include('partials/header'); -%>
<h1>
Compose
</h1>

<form action="/compose" method="post">
    <div class="form-group">
      <label for="exampleInputEmail1">Title</label>
      <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="postTitle">
    </div>
    <div class="form-group">
      <label for="postt">Post</label>
      <textarea class="form-control" name="postBody" id="postt" cols="30" rows="10"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
  </form>
<%- include('partials/footer'); -%>
